package Java_;

import java.util.Scanner;

/* Problem Number : 9498
 * Problem Name   : 시험 성적
 */
public class Q_16 {
	public static void main(String argv[]) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		if ( a % 4 == 0) {
			System.out.println("true");
		} else {
			System.out.println("false");
		}
	}

}
