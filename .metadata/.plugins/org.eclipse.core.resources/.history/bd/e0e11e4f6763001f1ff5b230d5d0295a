package Java_;

import java.util.Scanner;

/* Problem Number : 2525
 * Problem Name   : 오븐시계
 */
public class Q_19 {
	public static void main(String argv[]) {
		Scanner sc = new Scanner(System.in);
		int H,m,t = sc.nextInt();
//		int m = sc.nextInt();
//		int t = sc.nextInt();
		
		if ( m + t >= 60) {
			System.out.println(H+1 + " " + ((m+t)-60));
		}
		
	}
}
